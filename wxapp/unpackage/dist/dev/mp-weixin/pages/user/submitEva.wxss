.upload-box {
  background-color: #FFFFFF;
  padding: 24rpx;
}
.upload-box .upload {
  width: 160rpx;
  height: 160rpx;
  background: #FFFFFF;
  border-radius: 8rpx;
  border: 1px solid #E8E8E8;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: relative;
}
.upload-box .upload .add {
  width: 44rpx;
  height: 44rpx;
}
.upload-box .upload .close {
  position: absolute;
  right: -20rpx;
  top: -20rpx;
}
.upload-box .upload .close image {
  width: 44rpx;
  height: 44rpx;
}
.upload-box .upload .selected {
  width: 160rpx;
  height: 160rpx;
  border-radius: 8rpx;
}
